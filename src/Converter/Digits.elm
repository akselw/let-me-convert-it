module Converter.Digits exposing (..)

-- Number Systems


type RomanDigit
    = M
    | D
    | C
    | L
    | X
    | V
    | I


type DecimalDigit
    = One
    | Two
    | Three
    | Four
    | Five
    | Six
    | Seven
    | Eight
    | Nine
    | Zero


type BinaryDigit
    = BinaryZero
    | BinaryOne
